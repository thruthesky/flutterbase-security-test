rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {

    // 기본적으로 모든 읽기와 쓰기를 막는다.
    match /{document=**} {
      allow read: if false;
      allow write: if false;
    }
    // match /users/{user} {
    //   allow read, update, delete: if request.auth.uid == resource.id;
    //   allow create: if request.auth.uid != null;
    // }
  }
}